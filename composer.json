{
    "name": "kradwhite/config",
    "type": "library",
    "description": "Config",
    "keywords": [
        "kradwhite",
        "config",
        "configuration"
    ],
    "homepage": "https://github.com/kradwhite/config",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "kradwhite",
            "email": "kradwhite@mail.ru",
            "homepage": "https://github.com/kradwhite",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "kradwhite/language": "*"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-filesystem": "^1.0.2",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "kradwhite\\config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kradwhite\\config\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
